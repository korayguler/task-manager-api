{
  "name": "task-manager-api",
  "description": "Express.js task manager api",
  "main": "build/main.js",
  "scripts": {
    "start": "node -r tsconfig-paths/register dist/main.js",
    "dev": "ts-node-dev -r tsconfig-paths/register src/main.ts",
    "build": "tsc"
  },
  "dependencies": {
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "module-alias": "^2.2.2",
    "mongoose": "^6.0.11",
    "typescript": "^4.4.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^16.11.0",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.11.0"
  },
  "_moduleAliases": {
    "@controllers": "dist/controllers",
    "@services": "dist/services",
    "@middleware": "dist/middleware",
    "@models": "dist/models",
    "@routes": "dist/routes",
    "@errors": "dist/errors"
  }
}
